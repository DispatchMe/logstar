{
  "name": "logstar",
  "version": "1.0.0",
  "description": "Simple, opinionated, isomorphic logging.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dispatchme/logstar.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./lib",
    "prepublish": "rm -rf dist/** && ./node_modules/.bin/babel --ignore __tests__ lib --out-dir dist",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha --require mocha.conf.js lib/**/*-test.js",
    "test:watch": "npm run test:unit -- --watch"
  },
  "keywords": [
    "autoconfigure",
    "isomorphic",
    "logging",
    "loggly",
    "universal",
    "winston"
  ],
  "dependencies": {
    "winston": "^2.1.1",
    "winston-loggly": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "4.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "eslint": "1.9.x",
    "eslint-config-airbnb": "1.0.0",
    "expect": "1.13.x",
    "mocha": "2.x.x"
  }
}
